!font:
*font: -*-fixed-medium-r-*-*-13-*-*-*-*-*-iso10646-*

!fontstyle:
Xft.autohint: false
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb

!options:
xterm*metaSendsEscape: true
xterm*bellIsUrgent: true
xterm*borderWidth: 0
xterm*saveLines: 1000
URxvt*cursorBlink: true
URxvt*urgentOnBell: true
URxvt*visualBell: false
URxvt*skipBuiltinGlyphs: true
URxvt*borderless: true
URxvt*scrollBar: false
URxvt*saveLines: 1000

!links:
URxvt*perl-ext-common: matcher
URxvt*urlLauncher: firefox
URxvt*matcher.button: 1

!colors:
!scheme: pnevma
*foreground: #696969
*background: #282626
*color0: #363636
*color8: #424242
*color1: #C37561
*color9:  #D19485
*color2: #A0A57E
*color10: #B6B99D
*color3: #D1A375
*color11: #DEBC9C
*color4: #7985A3
*color12: #98A1B9
*color5: #AB716D
*color13: #BE918E
*color6: #98B9B1
*color14: #CBE6CB
*color7: #D0D0D0
*color15: #E6E6E6

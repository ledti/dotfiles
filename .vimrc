set tabstop=4
set shiftwidth=4
set expandtab
set smarttab
set number
set hlsearch
set ignorecase
set smartcase
syntax on
filetype plugin indent on
set t_Co=256
set background=dark
colorscheme elflord
map \ :noh <enter>
set printoptions=paper:letter,number:yes,left:5pc
set printfont="Envy Code R":h10
